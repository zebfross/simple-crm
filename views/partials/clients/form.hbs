<div class="row">
    <div class="col-xs-12 col-sm-12">
        <div class="box">
            <div class="box-header">
                <div class="box-name">
                    <i class="fa fa-search"></i>&#x9;&#x9;&#x9;&#x9;&#x9;
                    <span>Client Form
                    </span>
                </div>&#x9;&#x9;&#x9;&#x9;

                <div class="box-icons">
                    <a class="expand-link fa fa-expand" style="font-style: italic"></a>
                </div>&#x9;&#x9;&#x9;&#x9;

                <div class="no-move"></div>
            </div>&#x9;&#x9;&#x9;

            <div class="box-content">
                <h4 class="page-header">Client<i class="fa fa-pencil"></i></h4>&#x9;&#x9;&#x9;&#x9;
                <form method="POST" class="form-horizontal" id="client-form" name="client-form"
                    {{#if _id}}
                        action="/clients/{{_id}}/update"
                    {{else}}
                        action="/clients/"
                    {{/if}}>
                    <input id="_id" name="_id" type="hidden" value="{{_id}}">&#x9;&#x9;&#x9;&#x9;&#x9;

                    <div class="form-group">
                        <label class="col-sm-2 control-label">First Name
                        </label>&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;

                        <div class="col-sm-4">
                            <input class="form-control" data-placement="bottom" data-toggle="tooltip" id="name_first" name="name_first" placeholder="First Name" type="text" value="{{name_first}}">
                        </div>&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                        <label class="col-sm-2 control-label">Last Name
                        </label>&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;

                        <div class="col-sm-4">
                            <input class="form-control" data-placement="bottom" data-toggle="tooltip" id="name_last" name="name_last" placeholder="Last Name" type="text" value='{{name_last}}'>
                        </div>
                    </div>&#x9;&#x9;&#x9;&#x9;&#x9;

                    <div class="form-group">
                        <label class="col-sm-2 control-label">Email</label>&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;

                        <div class="col-sm-4">
                            <input class="form-control" data-bv-emailaddress="true" id="email" name="email" placeholder="Email" type="text" value='{{email}}'>
                        </div>&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                        <label class="col-sm-2 control-label">Phone</label>&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;

                        <div class="col-sm-4">
                            <input class="form-control" id="phones_0_number" name="phones[0].number" placeholder="Phone Number" type="text" value="{{phones.0.number}}">
                        </div>
                    </div>&#x9;&#x9;&#x9;&#x9;&#x9;

                    <div class="form-group">
                        <label class="col-sm-2 control-label">City</label>&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;

                        <div class="col-sm-4">
                            <input class="form-control" id="city" name="city" placeholder="City" type="text" value="{{city}}">
                        </div>&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                        <label class="col-sm-2 control-label">State</label>&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;

                        <div class="col-sm-4">
                            <input class="form-control" id="state" name="state" placeholder="State" type="text" value="{{state}}">
                        </div>
                    </div>&#x9;&#x9;&#x9;&#x9;&#x9;

                    <div class="form-group">
                        <label class="col-sm-2 control-label">Rating</label>&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;

                        <div class="col-sm-4">
                            <input class="form-control" id="rating" name="rating" placeholder="Rating" type="number" value="{{rating}}">
                        </div>
                    </div>&#x9;&#x9;&#x9;&#x9;&#x9;

                    <div class="form-group">
                        <label class="col-sm-2 control-label">Notes</label>&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;

                        <div class="col-sm-10">
                            <textarea id="notes" name="notes" style="width: 100%; height: 100px;" value="{{notes}}">{{notes}}</textarea>
                        </div>
                    </div>&#x9;&#x9;&#x9;&#x9;&#x9;

                    <div class="clearfix"></div>&#x9;&#x9;&#x9;&#x9;&#x9;

                    <div class="form-group">
                        <div class="col-sm-2">
                            <button class="btn btn-primary btn-label-left" type="submit">
                                <span class="fa fa-clock-o" style="font-style: italic"></span>&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Save</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
